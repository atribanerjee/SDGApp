
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        
        <ul class="breadcrumbs-alt none">
            @{
                if (ViewBag.lstbdcomb != null)
                    {
                    <!--lst[0] IS LINK. lst[1] SHOW. lst[2] CURRENT PAGE -->
                    List<string> lst = new List<string>();
                    lst = (List<string>)ViewBag.lstbdcomb;
                    if (lst.Count == 3)
                        {
                        <li><a href="/@lst[0]"> @lst[1] </a></li>
                        <li><a href="" class="current"> @lst[2] </a></li>

                        }

                    }
                else
                    {
                    <li>
                        <a href="#" class="current">Dashboard</a>
                    </li>
                    }
            }
        </ul>

    </div>
</div>
